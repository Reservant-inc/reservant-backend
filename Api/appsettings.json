{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },
  "AllowedHosts": "*",
  "FileUploads": {
    "SavePath": "/app/uploads/",
    "ServePath": "/uploads",
    "MaxSizeKb": 1024
  },
  "ConnectionStrings": {
    "LogDb": "Data Source=log.db"
  }
}
