meta {
  name: Restaurant Employee Access Denied
  type: http
  seq: 5
}

put {
  url: {{host}}/menu-items/:id
  body: json
  auth: bearer
}

params:path {
  id: {{randomMenuItemId}}
}

auth:bearer {
  token: {{employee}}
}

body:json {
  {
