    "name": "{{menuName}}",
    "alternateName": "{{menuName}}",
    "alcoholPercentage": {{randomDouble0to3}},
    "photo": "test-jd.png",
      "ingredients": []
  }
}

tests {
  test("Status code is 403", function () {
      expect(res.getStatus()).to.equal(403);
  });
}
